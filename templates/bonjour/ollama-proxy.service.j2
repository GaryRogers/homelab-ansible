<?xml version="1.0" standalone='no'?><!--*-nxml-*-->
<!DOCTYPE service-group SYSTEM "avahi-service.dtd">

<!-- 
Ollama API Proxy Service Advertisement for {{ hostname_override }}
Generated by Ansible on {{ ansible_date_time.iso8601 }}
-->

<service-group>
  <name replace-wildcards="yes">Ollama API Proxy on %h</name>
  <service>
    <type>_ollama._tcp</type>
    <port>11434</port>
    <txt-record>txtvers=1</txt-record>
    <txt-record>path=/</txt-record>
    <txt-record>description=Ollama API with WoL Proxy</txt-record>
    <txt-record>features=wake-on-lan,reverse-proxy</txt-record>
  </service>
</service-group>