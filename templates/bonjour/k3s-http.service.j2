<?xml version="1.0" standalone='no'?><!--*-nxml-*-->
<!DOCTYPE service-group SYSTEM "avahi-service.dtd">

<!-- 
K3s Ingress HTTP Service Advertisement for {{ hostname_override }}
Generated by Ansible on {{ ansible_date_time.iso8601 }}
-->

<service-group>
  <name replace-wildcards="yes">K3s Web Services on %h</name>
  <service>
    <type>_http._tcp</type>
    <port>80</port>
    <txt-record>txtvers=1</txt-record>
    <txt-record>path=/</txt-record>
    <txt-record>description=K3s Ingress Router</txt-record>
  </service>
</service-group>