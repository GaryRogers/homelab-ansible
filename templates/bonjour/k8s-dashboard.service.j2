<?xml version="1.0" standalone='no'?><!--*-nxml-*-->
<!DOCTYPE service-group SYSTEM "avahi-service.dtd">

<!-- 
Kubernetes Dashboard Service Advertisement for {{ hostname_override | default(ansible_hostname) }}
Generated by Ansible on {{ ansible_date_time.iso8601 }}
-->

<service-group>
  <name replace-wildcards="yes">Kubernetes Dashboard on %h</name>
  <service>
    <type>_https._tcp</type>
    <port>{{ dashboard_port | default(8443) }}</port>
    <txt-record>txtvers=1</txt-record>
    <txt-record>path=/</txt-record>
    <txt-record>description=Kubernetes Dashboard Web UI</txt-record>
    <txt-record>namespace={{ dashboard_namespace | default('kubernetes-dashboard') }}</txt-record>
    <txt-record>version=helm</txt-record>
  </service>
</service-group>